<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calorieapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_local_dining_black"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_local_dining_black"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".History.ListActivity"></activity>
        <activity android:name=".DetailProductActivity" />
        <activity android:name=".History.HistoryActivity" />
        <activity android:name=".CategoriesActivity" />
        <activity android:name=".SearchPage.SearchActivity" />
        <activity android:name=".MyProfile.MyProfile" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>