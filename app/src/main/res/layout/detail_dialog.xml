<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/fiberTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/black"
        android:layout_alignStart="@+id/fatTV"
        android:layout_alignTop="@+id/textView9"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-2dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/textView"
        android:textColor="@android:color/black"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="25dp"
        android:text="Product name" />

    <TextView
        android:id="@+id/textView2"
        android:textColor="@android:color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_alignStart="@+id/textView"
        android:layout_marginStart="0dp"
        android:layout_marginTop="22dp"
        android:text="Product weight" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:textColor="@android:color/black"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:layout_marginStart="0dp"
        android:layout_marginTop="22dp"
        android:text="Calories" />

    <TextView
        android:id="@+id/name_productTV"
        android:layout_width="154dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView"
        android:textColor="@android:color/black"
        android:layout_marginStart="30dp"
        android:layout_marginTop="-1dp"
        android:layout_toEndOf="@+id/textView"
        android:text="TextView" />

    <TextView
        android:id="@+id/weight_productTV"
        android:layout_width="wrap_content"
        android:layout_height="29dp"
        android:textColor="@android:color/black"
        android:layout_alignStart="@+id/name_productTV"
        android:layout_alignTop="@+id/textView2"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-2dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/energyTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/weight_productTV"
        android:layout_alignTop="@+id/textView3"
        android:layout_marginStart="0dp"
        android:textColor="@android:color/black"
        android:layout_marginTop="-1dp"
        android:text="TextView" />

    <Button
        android:id="@+id/positiveButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView9"
        android:textColor="@android:color/black"
        android:layout_alignStart="@+id/textView3"
        android:layout_alignEnd="@+id/name_productTV"
        android:layout_marginStart="91dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="95dp"
        android:background="@drawable/buttom_look"
        android:layout_marginBottom="15dp"
        android:text="Ok" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/weight_productTV"
        android:textColor="@android:color/black"
        android:layout_marginStart="8dp"
        android:layout_marginTop="2dp"
        android:layout_toEndOf="@+id/weight_productTV"
        android:text="g" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/energyTV"
        android:layout_marginStart="3dp"
        android:layout_marginTop="1dp"
        android:textColor="@android:color/black"
        android:layout_toEndOf="@+id/energyTV"
        android:text="kcal" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView3"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginStart="0dp"
        android:textColor="@android:color/black"
        android:layout_marginTop="22dp"
        android:text="Protein" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:layout_marginStart="0dp"
        android:layout_marginTop="22dp"
        android:text="Carbs"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_alignStart="@+id/textView7"
        android:layout_marginStart="0dp"
        android:layout_marginTop="22dp"
        android:text="Fat"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:layout_alignStart="@+id/textView8"
        android:layout_marginStart="0dp"
        android:textColor="@android:color/black"
        android:layout_marginTop="22dp"
        android:text="Fiber" />

    <TextView
        android:id="@+id/proteinTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/energyTV"
        android:layout_alignTop="@+id/textView6"
        android:textColor="@android:color/black"
        android:layout_marginStart="0dp"
        android:layout_marginTop="4dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/carbsTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/proteinTV"
        android:layout_alignTop="@+id/textView7"
        android:textColor="@android:color/black"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-1dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/fatTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/black"
        android:layout_alignStart="@+id/carbsTV"
        android:layout_alignTop="@+id/textView8"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-2dp"
        android:text="TextView" />
</RelativeLayout>